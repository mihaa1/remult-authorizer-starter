{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"module": "Node16",
		"moduleResolution": "Node16",
		"allowImportingTsExtensions": false,
		"resolveJsonModule": true,
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"noEmit": false,
		"outDir": "dist_server",
		"rootDir": "src"
	},
	"include": ["src/server/index.ts"]
}
